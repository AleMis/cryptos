create table api_keys(id int not null auto_increment, api_key varchar(255), api_secret_key varchar(255), client_id varchar(255), exchange varchar(255), primary key (id));

create table individual_strategy(id int not null auto_increment, instrument_type VARCHAR(20), instrument VARCHAR(20), position VARCHAR(20), exchange VARCHAR(20), price DECIMAL(12,6), price_achieved BOOLEAN, timestamp LONG, PRIMARY KEY (id));
